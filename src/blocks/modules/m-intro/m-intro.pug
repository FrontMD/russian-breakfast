-
    const defaultMIntroProps = {
        classes: ''
    }

mixin m-intro(props = defaultMIntroProps)
    -
        const checkProps = {
            classes: props.classes ? props.classes : defaultMIntroProps.classes
        }

    section.m-intro(class= checkProps.classes)
        .m-container.m-intro__container
            .m-intro__img
                img(src="./img/home_about.png" alt="")
            .m-intro__logo.logo
                img(src="./img/logo.svg" alt="Логотип Русский завтрак")
            .m-intro__title ЗАВТРАК КАК ТРАДИЦИЯ, ЗАВТРАК КАК НАСТОЯЩИЙ ПРАЗДНИК